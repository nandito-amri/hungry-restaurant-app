{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-console": "off",
        "max-len": "off",
        "no-restricted-globals": "off",
        "import/no-extraneous-dependencies": "off",
        "linebreak-style": "off",
        "no-unused-vars":"off",
        "no-undef": "off",
        "consistent-return": "off",
        "no-prototype-builtins": "off"
    },
    "plugins": [
        "jasmine"
    ]
}
